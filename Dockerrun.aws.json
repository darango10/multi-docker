{
    "AWSEBDockerrunVersion":"2",
    "containerDefinitions":[
        {
            "name":"client",
            "image":"darango10/multi-client",
            "hostname":"client",
            "essential": false,
            "memory":128
        },
        {
            "name":"server",
            "image":"darango10/multi-server",
            "hostname":"api",
            "essential": false,
            "memory":128
        },
        {
            "name":"worker",
            "image":"darango10/multi-worker",
            "hostname":"worker",
            "essential": false,
            "memory":128
        },
        {
            "name":"nginx",
            "image":"darango10/multi-nginx",
            "essential": true,
            "portMappings":[
                {
                    "host":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"],
            "memory":128
        }
    ]
}